{
  "name": "whydonate-web-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng run serve:ssr",
    "start-cloudflare-functions": "npx wrangler pages dev -- npm run start:nl",
    "start:en": "ng serve --configuration en",
    "start:nl": "ng serve --configuration nl",
    "start:es": "ng serve --configuration es",
    "start:de": "ng serve --configuration de",
    "start:fr": "ng serve --configuration fr",
    "build": "ng build --aot --localize --configuration staging && npm run postbuild",
    "build-prod": "ng build --aot --output-hashing=all --localize --configuration production && npm run postbuild",
    "build-staging": "npm run build:ssr",
    "build-features": "npm run build:ssr",
    "build-master": "ng build --aot --output-hashing=all --localize --configuration master && npm run postbuild",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc": "npx compodoc -p tsconfig.doc.json --minimal",
    "docker:build": "docker build --no-cache -t whydonatenl/whydonate-web-portal-image:latest .",
    "extract-i18n": "ng extract-i18n --format json --out-file src/i18n/messages.en-base.json",
    "e2e:ci": "ng run whydonate-web-portal:e2e-ci",
    "e2e:coverage": "npx nyc report --reporter=lcov --reporter=text-summary",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "run:cypress": "node_modules\\.bin\\cypress open",
    "postbuild": "cp .dist/whydonate-web-portal/browser/en/index.html ./dist/whydonate-web-portal",
    "package-clean": "npm i --force",
    "dev:ssr": "ng run whydonate-web-portal:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --aot --localize --configuration staging && npm run postbuild",
    "prerender": "ng run whydonate-web-portal:prerender",
    "serve:ssr-en": "node dist/whydonate-web-portal/server/en/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.5",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^17.0.5",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@angular/platform-server": "17.0.5",
    "@angular/router": "^17.0.5",
    "@angular/ssr": "^17.0.5",
    "@babel/runtime": "^7.22.3",
    "@ctrl/tinycolor": "^3.4.1",
    "@egjs/hammerjs": "^2.0.17",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@planetscale/database": "^1.11.0",
    "@sentry/angular": "^7.80.0",
    "@sentry/integrations": "^7.80.0",
    "@sentry/tracing": "^7.80.0",
    "angularx-qrcode": "^15.0.0",
    "blob-polyfill": "^7.0.20220408",
    "express": "^4.15.2",
    "heic2any": "^0.0.3",
    "html2canvas": "^1.4.1",
    "jsdom": "^23.0.1",
    "jspdf": "^2.5.1",
    "localstorage-polyfill": "^1.0.1",
    "mailosaur": "^8.6.1",
    "mat-table-exporter": "^15.0.0",
    "moment": "^2.29.4",
    "ng-image-slider": "^7.0.1",
    "ngx-cookie-service": "^15.0.0",
    "ngx-image-cropper": "^7.1.1",
    "ngx-moment": "5.0.0",
    "ngx-quill": "^22.1.0",
    "ngx-sharebuttons": "^9.0.0",
    "node": "^18.18.0",
    "quill": "^1.3.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.6.2",
    "xmlbuilder": "^15.1.1",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@angular/localize": "^17.0.5",
    "@cloudflare/workers-types": "^3.16.0",
    "@compodoc/compodoc": "^1.1.19",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/grep": "^3.1.0",
    "@cypress/schematic": "^2.4.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.2",
    "@types/node": "^18.18.0",
    "@types/quill": "^1.3.10",
    "cypress": "^13.3.3",
    "cypress-file-upload": "^5.0.8",
    "esbuild": "^0.17.19",
    "istanbul-lib-coverage": "^3.2.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "typescript": "^5.2.2",
    "webpack": "^5.75.0",
    "wrangler": "^3.15.0"
  },
  "nyc": {
    "excludeAfterRemap": false,
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "cypress/",
      "assets/",
      "test/"
    ],
    "all": true
  }
}